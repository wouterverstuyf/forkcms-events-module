<?xml version="1.0" encoding="UTF-8"?>
<module>
	<name>Events</name>
	<version>1.0.0</version>
	<requirements>
		<minimum_version>versionname</minimum_version>
	</requirements>
	<description>
		<![CDATA[This module makes it possible to add and manage events.]]>
	</description>
	<authors>
		<author>
			<name><![CDATA[Wouter Verstuyf]]></name>
			<url><![CDATA[http://www.webflow.be]]></url>
		</author>
	</authors>
	<events>
		<event application="backend" name="after_add"><![CDATA[Triggered when a Events is added.]]></event>
		<event application="backend" name="after_delete"><![CDATA[Triggered when a Events is deleted.]]></event>
		<event application="backend" name="after_edit"><![CDATA[Triggered when a Events is edited.]]></event>
		<event application="backend" name="after_saved_settings"><![CDATA[Triggered when settings are saved.]]></event>
	</events>
</module>